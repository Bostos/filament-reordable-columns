{
    "name": "bostos/filament-reorderable-columns",
    "description": "This is my package filament-reorderable-columns",
    "keywords": [
        "Bostos",
        "laravel",
        "filament",
        "filament-tables",
        "filament-reorderable-columns"
    ],
    "homepage": "https://github.com/bostos/filament-reorderable-columns",
    "support": {
        "issues": "https://github.com/bostos/filament-reorderable-columns/issues",
        "source": "https://github.com/bostos/filament-reorderable-columns"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bostos",
            "email": "nikolast_metal@hotmail.com",
            "role": "PHP Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/tables": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Bostos\\FilamentReorderableColumns\\": "src/",
            "Bostos\\FilamentReorderableColumns\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bostos\\FilamentReorderableColumns\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bostos\\FilamentReorderableColumns\\FilamentReorderableColumnsServiceProvider"
            ],
            "aliases": {
                "FilamentReorderableColumns": "Bostos\\FilamentReorderableColumns\\Facades\\FilamentReorderableColumns"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}